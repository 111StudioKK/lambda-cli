{
  "name": "lambda-cli",
  "version": "0.5.7",
  "description": "A React Web boilerplate / scaffolding tool used at 111Studio",
  "main": "index.js",
  "bin": {
    "lambda": "cli/lambda.js"
  },
  "scripts": {
    "test": "mocha ./.mochaSetup.js ./src/**/*.spec.js --compilers js:./.mochaImport.js -w -c -G",
